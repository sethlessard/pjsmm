{
  "name": "mm-ts",
  "version": "1.0.0",
  "description": "CLI application to manage dependencies required by typescript sub-projects in a main project package.json",
  "bin": {
    "mm-ts": "./lib/index.js"
  },
  "main": "./lib/index.js",
  "scripts": {
    "compile": "rollup -c",
    "debug": "yarn compile && node --inspect-brk ./lib/index.js",
    "start": "yarn compile && node ./lib/index.js",
    "clean": "rimraf lib/",
    "lint": "eslint src/; eslint test/",
    "test": "yarn lint; yarn test:unit; yarn test:integration; yarn test:e2e",
    "test:unit": "nyc mocha --require ts-node/register --require source-map-support/register --reporter mocha-junit-reporter --extensions ts -- 'test/unit/**/*.ts'",
    "debug:unit": "mocha inspect --require ts-node/register --extensions ts --watch --watch-files src/,test/ -- 'test/unit/**/*.ts'",
    "test:unit:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files src/,test/ -- 'test/unit/**/*.ts'",
    "test:integration": "nyc mocha --require ts-node/register --require source-map-support/register --reporter mocha-junit-reporter --extensions ts -- 'test/integration/**/*.ts'",
    "debug:integration": "mocha --inspect --require ts-node/register --extensions ts --watch --watch-files src/,test/ -- 'test/integration/**/*.ts'",
    "test:integration:watch": "mocha --require ts-node/register --extensions ts --watch --watch-files src/,test/ -- 'test/integration/**/*.ts'",
    "test:e2e": "yarn compile && mocha --require ts-node/register --extensions ts --timeout 120000 -- 'test/e2e/**/*.ts'",
    "test:e2e:ci": "yarn compile && mocha --require ts-node/register --extensions ts --timeout 120000 --reporter mocha-junit-reporter -- 'test/e2e/**/*.ts'"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/coverage/*"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "all": true
  },
  "author": "Seth Lessard",
  "license": "MIT",
  "dependencies": {
    "boxen": "5.0.0",
    "chalk": "4.1.0",
    "lodash": "4.17.21",
    "reflect-metadata": "0.1.13",
    "regenerator-runtime": "0.13.7",
    "tsyringe": "4.4.0",
    "uuid": "8.3.2",
    "yargs": "16.2.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@rollup/plugin-node-resolve": "11.2.0",
    "@types/chai": "4.2.15",
    "@types/lodash": "4.14.168",
    "@types/mocha": "8.2.1",
    "@types/rimraf": "3.0.0",
    "@types/uuid": "8.3.0",
    "@types/yargs": "16.0.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@wessberg/rollup-plugin-ts": "^1.3.8",
    "chai": "4.3.0",
    "eslint": "7.21.0",
    "mocha": "8.3.0",
    "mocha-junit-reporter": "2.0.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "rollup": "2.40.0",
    "rollup-plugin-add-shebang": "0.3.1",
    "source-map-support": "0.5.19",
    "ts-node": "9.1.1",
    "typescript": "4.2.2"
  }
}
